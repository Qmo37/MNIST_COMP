    "        self.model = nn.Sequential(",
    "            nn.Linear(784, 1024),",
    "            nn.LeakyReLU(0.2),",
    "            nn.Dropout(0.3),",
    "            nn.Linear(1024, 512),",
    "            nn.LeakyReLU(0.2),",
    "            nn.Dropout(0.3),",
    "            nn.Linear(512, 256),",
    "            nn.LeakyReLU(0.2),",
    "            nn.Dropout(0.3),",
    "            nn.Linear(256, 1),",
    "            nn.Sigmoid()",
    "        )",
    "    ",
    "    def forward(self, img):",
    "        return self.model(img.view(-1, 784))",
    "",
